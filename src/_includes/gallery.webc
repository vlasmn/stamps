<div class="gallery">
<div class="grid-sizer"></div>
{% for country, years in collections.gallery %}
    {% for year, images in years %}
    {% for image in images %}
        <div class="gallery-item">
        <img src="{{ image.url }}" alt="{{ image.alt }}">
        </div>
    {% endfor %}
    {% endfor %}
{% endfor %}
</div>
